  },
  "build": {
    "appId": "com.cosmosgenius.leveladmin",
    "category": "public.app-category.developer-tools",
    "asar": false
  },
  "scripts": {
    "dev": "cross-env HOT=1 NODE_ENV=development webpack-dev-server",
